{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "prefer-const": true,
    "quotemark": [true, "single"],
    "trailing-comma": false,
    "semicolon": false,
    "ordered-imports": false
  }
}