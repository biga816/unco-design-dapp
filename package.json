{
  "name": "unco-design-dapp",
  "version": "1.0.0",
  "description": "This project is for recording the Unco in a distributed network.",
  "author": "akihiro.tanaka",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "rimraf dist && tsc --module CommonJS && node ./scripts/copy-statics.js && nuxt build",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "generate": "nuxt generate",
    "lint": "tslint --project ./ --config ./",
    "precommit": "npm run lint",
    "truffle": "truffle develop",
    "test": "jest",
    "firebase:build": "npm run firebase:clear && npm run firebase:copy",
    "firebase:install": "rimraf functions/node_modules && npm --prefix ./functions install ./functions",
    "firebase:serve": "firebase serve --only hosting,functions",
    "firebase:deploy": "firebase deploy",
    "firebase:clear": "rimraf functions/.nuxt && rimraf functions/server && mkdirp functions/.nuxt && mkdirp functions/server",
    "firebase:copy": "cp -R .nuxt functions && cp -R dist/server functions && cp package.json functions && cp package-lock.json functions"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/pwa": "^2.6.0",
    "buffer": "^5.2.1",
    "chart.js": "^2.7.3",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "express": "^4.16.4",
    "firebase-admin": "^7.1.1",
    "firebase-functions": "^2.2.1",
    "ipfs-api": "^26.1.2",
    "nuxt": "^2.4.0",
    "nuxt-ts": "^2.4.3",
    "openzeppelin-solidity": "^2.1.3",
    "pts": "^0.7.0",
    "rxjs": "^6.4.0",
    "truffle-contract": "^4.0.9",
    "vue-awesome-swiper": "^3.1.3",
    "vue-chartjs": "^3.4.0",
    "vue-property-decorator": "^7.3.0",
    "vuetify": "^1.5.0",
    "vuetify-loader": "^1.2.0",
    "vuex-persistedstate": "^2.5.4",
    "web3": "^1.0.0-beta.37"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/consola": "^1.0.0",
    "@types/express": "^4.16.1",
    "@types/web3": "^1.0.18",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "firebase-tools": "^6.5.0",
    "jest": "^23.6.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "prettier": "1.14.3",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "truffle": "^5.0.8",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "tslint-loader": "^3.5.4",
    "vue-jest": "^3.0.2"
  }
}
